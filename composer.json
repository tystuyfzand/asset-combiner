{
    "name": "tystuyfzand/asset-combiner",
    "description": "An asset combiner for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tyler",
            "email": "tyler@tystuyfzand.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.0|~6.0|~7.0|~8.0",
        "assetic/framework": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "AssetCombiner\\": "src/"
        }
    },
    "suggest": {
        "matthiasmullie/minify": "Minify Javascript output",
        "wikimedia/less.php": "Less PHP Compiler",
        "scssphp/scssphp": "SCSS PHP Compiler"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AssetCombiner\\AssetCombinerServiceProvider"
            ],
            "aliases": {
                "CombineAssets": "AssetCombiner\\Facades\\CombineAssets"
            }
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tystuyfzand/assetic.git"
        }
    ]
}
